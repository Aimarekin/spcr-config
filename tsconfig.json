{
    "compilerOptions": {
      "target": "ES2017",
      "module": "commonjs",
      "resolveJsonModule": true,
      "outDir": "../_dist",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "skipLibCheck": true,
      "jsx": "react",
      "jsxFactory": "Spicetify.React.createElement",
      "jsxFragmentFactory": "Spicetify.React.createFragment",
    }
  }