{
    "compilerOptions": {
      "target": "ES2016",
      "lib": ["ES2016", "DOM"],
      "outDir": "dist",
      "rootDir": "src",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "jsx": "react",
      "jsxFactory": "Spicetify.React.createElement",
      "jsxFragmentFactory": "Spicetify.React.createFragment"
    },
    "include": [ "src/**/*" ]
  }